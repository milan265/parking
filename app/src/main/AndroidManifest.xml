<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="android.rs.ac.parking">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".RacunModelActivity"></activity>
        <activity android:name=".SviRacuniActivity" />
        <activity android:name=".GarazeActivity" />
        <activity android:name=".RacunActivity" />
        <activity android:name=".KodActivity" />
        <activity android:name=".PocetnaActivity" />
        <activity android:name=".RegistracijaActivity" />
        <activity android:name=".PrijavaActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>